public class Par<F, S> { 
    // Aquí tenemos dos campos: uno para el primer elemento del par (F) 
    // y otro para el segundo (S).
    private F primero;
    private S segundo;

    // Constructor que toma el primer y segundo elementos y los inicializa.
    public Par(F primero, S segundo) {
        this.primero = primero;  // Asignamos el primer valor
        this.segundo = segundo;  // Asignamos el segundo valor
    }

    // Método para obtener el primer elemento.
    public F getPrimero() {
        return primero;  // Devolvemos el valor del primer elemento
    }

    // Método para cambiar el primer elemento.
    public void setPrimero(F primero) {
        this.primero = primero;  // Le damos un nuevo valor al primer elemento
    }

    // Método para obtener el segundo elemento.
    public S getSegundo() {
        return segundo;  // Devolvemos el valor del segundo elemento
    }

    // Método para cambiar el segundo elemento.
    public void setSegundo(S segundo) {
        this.segundo = segundo;  // Le damos un nuevo valor al segundo elemento
    }

    // Método para devolver el par en formato String.
    @Override
    public String toString() {
        // Aquí simplemente estamos formando una cadena bonita con los valores del par.
        return "(Primero: " + primero + ", Segundo: " + segundo + ")";
    }
}
